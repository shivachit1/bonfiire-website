{
  "name": "bonfiire",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "jquery": "^3.5.1",
    "lucide-react": "^0.575.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-icons": "^5.4.0",
    "react-router-dom": "^7.1.3",
    "react-scripts": "^5.0.1",
    "react-scroll": "^1.8.0",
    "typescript": "^3.2.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build:dev": "react-scripts build && mkdir -p ./build/.well-known && cp -R ./public/.well-known/development/* ./build/.well-known/ && cp ./public/.well-known/development/apple-app-site-association ./build/apple-app-site-association",
    "build:staging": "react-scripts build && mkdir -p ./build/.well-known && cp -R ./public/.well-known/staging/* ./build/.well-known/ && cp ./public/.well-known/staging/apple-app-site-association ./build/apple-app-site-association",
    "build:prod": "react-scripts build && mkdir -p ./build/.well-known && cp -R ./public/.well-known/production/* ./build/.well-known/ && cp ./public/.well-known/production/apple-app-site-association ./build/apple-app-site-association",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
